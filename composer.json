{
    "name": "aerian/een-project",
    "description": "EEN Project",
    "type": "project",
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "Console\\": "module/Console/src",
            "Search\\": "module/Search/src",
            "ConsoleTest\\": "module/Console/test",
            "SearchTest\\": "module/Search/test"
        }
    },
    "keywords": [
        "api",
        "framework",
        "zf2"
    ],
    "homepage": "http://aerian.com/",
    "support": {
        "email": "aerian@aerian.com",
        "source": "https://devops.innovateuk.org/code-repository/projects/EEN/repos/een-elasticsearch/",
        "issues": "https://devops.innovateuk.org/issue-tracking/secure/Dashboard.jspa"
    },
    "config": {
        "process-timeout": 5000
    },
    "require": {
        "php": "^5.5 || ^7.0",
        "zendframework/zendframework": "^2.5.3",
        "zfcampus/zf-apigility": "~1.1",
        "zfcampus/zf-apigility-documentation": "^1.0.5",
        "zfcampus/zf-development-mode": "~2.0",
        "elasticsearch/elasticsearch": "^2.2",
        "fzaninotto/faker": "^1.6"
    },
    "require-dev": {
        "zendframework/zftool": "dev-master",
        "zendframework/zend-developer-tools": "dev-master",
        "zfcampus/zf-deploy": "~1.0"
    },
    "suggest": {
        "zfcampus/zf-apigility-doctrine": "zfcampus/zf-apigility-doctrine ~1.0 to create Doctrine-Connected REST services",
        "zfcampus/zf-http-cache": "zfcampus/zf-http-cache ~1.0 to add HTTP caching to your API",
        "zfr/zfr-cors": "zfr/zfr-cors ~1.2 to add CORS support to your API"
    }
}
